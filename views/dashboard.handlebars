   {{!-- The dashboard should display each blog post belonging to the user and delete button --}}
 <div>
    <p>Welcome to your Dashboard</p>
    <p>Check out all your Blogs!</p>
</div>

<div class="row mt-4">
  <div class="col-md-6">
    <h3>Create a New Blog Post:</h3>

    <form class="form new-project-form">
      <div class="form-group">
        <label for="blog-title">Blog Title:</label>
        <input class="form-input" type="text" id="blog-title" name="blog-title" />
      </div>
      <div class="form-group">
        <label for="blog-entry">Blog Entry:</label>
        <input class="form-input" type="text" id="blog-entry" name="blog-entry" />
      </div>
      <div class="form-group">
        <button type="submit" class="btn btn-primary">Create</button>
      </div>
    </form>
  </div>


  <div class="col-md-6 blog-list">
    <h3>Your Current Blogs:</h3>
  {{#each userblogs as |asdf|}}
  {{> SINGLEENTRY}}
    {{/each}}
  </div>

</div>

<script src="./js/profile.js"></script>